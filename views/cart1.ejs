    <main>
        <div class="container">
            <ul id="cart-items">
            </ul>
            <div class="total-section">
                <h2 id="total"></h2>
            </div>
            <div class="actions">
              <form id="continue-btn" method="POST" action="/index1">
              <button type="submit" name="continue" value="continue">Continue Shopping</button>
              </form>
                <form id="proceed-btn" method="POST" action="/cart1">
                  <button type="submit" name="proceed" value="proceed">Proceed To Checkout</button>
                </form>
              </div>
        </div>
    </main>
